class Essen{
  String geschmack;
  int menge;
  
  void zeigeEigenschaften(){
    print(this.geschmack);
    print(this.menge);
  }
}

main(){
  Essen kiwi = Essen();
  kiwi.geschmack = "k√∂stlich";
  kiwi.menge = 100000;
  kiwi.zeigeEigenschaften();
  Essen banane = Essen();
  banane.geschmack = "fruchtig";
  banane.menge = 0;
  banane.zeigeEigenschaften();
}
